{
  "name": "bucklescript-tea-starter-kit",
  "version": "0.1.0",
  "scripts": {
    "clean": "bsb -clean",
    "clean:all": "bsb -clean-world",
    "build:bsb": "bsb -make-world",
    "build": "run-s build:bsb build:js",
    "build:js": "webpack --config webpack.prod.js --optimize-minimize",
    "watch:bsb": "bsb -make-world -w",
    "start": "run-p watch:bsb webpack",
    "webpack": "webpack-dev-server --content-base release"
  },
  "keywords": [
    "Bucklescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "bs-platform": "^2.0.0",
    "bucklescript-tea": "git://github.com/tcoopman/bucklescript-tea.git#patch-3",
    "npm-run-all": "^4.1.0",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.8.1"
  }
}
